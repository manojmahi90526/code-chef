import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner s=new Scanner(System.in);
		int A=s.nextInt();
		int B=s.nextInt();
		if(A>B){
		    System.out.println(A-B);
		}
		else
		System.out.println("0");

	}
}
