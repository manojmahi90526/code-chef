import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
			Scanner s=new Scanner(System.in);
		int H=s.nextInt();
		if(H==8){
		System.out.println("perfect");
		}
		else{
		if(H>8){
		System.out.println("MORE");
		}
		else
		System.out.println("LESS");
		}
		

	}
}
